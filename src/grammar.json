{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "lprog",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_element"
      }
    },
    "_element": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "expression"
        },
        {
          "type": "SYMBOL",
          "name": "global_macro"
        },
        {
          "type": "SYMBOL",
          "name": "code_macro"
        },
        {
          "type": "SYMBOL",
          "name": "include_statement"
        },
        {
          "type": "SYMBOL",
          "name": "macro_name"
        }
      ]
    },
    "global_macro": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "symbol",
          "content": {
            "type": "SYMBOL",
            "name": "macro_symbol"
          }
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "macro_name"
          }
        },
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_element"
          }
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "code_macro": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "symbol",
          "content": {
            "type": "SYMBOL",
            "name": "macro_symbol"
          }
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "macro_name"
          }
        },
        {
          "type": "FIELD",
          "name": "body",
          "content": {
            "type": "SYMBOL",
            "name": "_l_expression"
          }
        }
      ]
    },
    "include_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "symbol",
          "content": {
            "type": "SYMBOL",
            "name": "macro_symbol"
          }
        },
        {
          "type": "FIELD",
          "name": "file_name",
          "content": {
            "type": "SYMBOL",
            "name": "string"
          }
        }
      ]
    },
    "_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "l_expression"
        },
        {
          "type": "SYMBOL",
          "name": "expression"
        }
      ]
    },
    "expression": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "symbol",
            "content": {
              "type": "SYMBOL",
              "name": "expr_symbol"
            }
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "FIELD",
                "name": "body",
                "content": {
                  "type": "SYMBOL",
                  "name": "_expression"
                }
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    },
    "l_expression": {
      "type": "SYMBOL",
      "name": "_l_expression"
    },
    "_l_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "l_function"
        },
        {
          "type": "SYMBOL",
          "name": "l_application"
        },
        {
          "type": "SYMBOL",
          "name": "l_variable"
        },
        {
          "type": "SYMBOL",
          "name": "l_macro"
        }
      ]
    },
    "l_function": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\\"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "REPEAT",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "FIELD",
                    "name": "variable",
                    "content": {
                      "type": "SYMBOL",
                      "name": "l_variable"
                    }
                  },
                  {
                    "type": "STRING",
                    "value": ","
                  }
                ]
              }
            },
            {
              "type": "FIELD",
              "name": "variable",
              "content": {
                "type": "SYMBOL",
                "name": "l_variable"
              }
            }
          ]
        },
        {
          "type": "STRING",
          "value": "."
        },
        {
          "type": "FIELD",
          "name": "body",
          "content": {
            "type": "SYMBOL",
            "name": "_l_expression"
          }
        }
      ]
    },
    "l_application": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "FIELD",
          "name": "function",
          "content": {
            "type": "SYMBOL",
            "name": "_l_expression"
          }
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "FIELD",
                "name": "arg",
                "content": {
                  "type": "SYMBOL",
                  "name": "_l_expression"
                }
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "l_variable": {
      "type": "FIELD",
      "name": "name",
      "content": {
        "type": "SYMBOL",
        "name": "var_name"
      }
    },
    "l_macro": {
      "type": "FIELD",
      "name": "name",
      "content": {
        "type": "SYMBOL",
        "name": "macro_name"
      }
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "//"
              },
              {
                "type": "PATTERN",
                "value": "(\\\\+(.|\\r?\\n)|[^\\\\\\n])*"
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "/*"
              },
              {
                "type": "PATTERN",
                "value": "[^*]*\\*+([^/*][^*]*\\*+)*"
              },
              {
                "type": "STRING",
                "value": "/"
              }
            ]
          }
        ]
      }
    },
    "var_name": {
      "type": "PATTERN",
      "value": "[a-z_]+"
    },
    "macro_name": {
      "type": "PATTERN",
      "value": "[A-Z_]+"
    },
    "string": {
      "type": "PATTERN",
      "value": "\"[a-zA-Z0-9/]*\""
    },
    "expr_symbol": {
      "type": "PATTERN",
      "value": "[#$&]"
    },
    "macro_symbol": {
      "type": "PATTERN",
      "value": "[%@*]"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s|\\\\\\r?\\n"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}